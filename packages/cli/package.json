{
  "name": "@wocat/cli",
  "type": "module",
  "version": "0.0.0",
  "description": "CLI for wocat",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/wocat#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/wocat.git"
  },
  "bugs": "https://github.com/byronogis/wocat/issues",
  "keywords": [],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "wocat": "./bin/index.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@wocat/core": "workspace:*",
    "chokidar": "^3.6.0",
    "citty": "^0.1.6",
    "colorette": "^2.0.20",
    "consola": "^3.4.0",
    "fs-extra": "^11.3.0",
    "pathe": "^2.0.2",
    "perfect-debounce": "^1.0.0",
    "tinyglobby": "^0.2.10"
  },
  "devDependencies": {}
}
