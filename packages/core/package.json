{
  "name": "@wocat/core",
  "type": "module",
  "version": "0.0.1",
  "description": "Core for wocat",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/wocat#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/wocat.git"
  },
  "bugs": "https://github.com/byronogis/wocat/issues",
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "@types/semver": "^7.5.8",
    "c12": "^2.0.1",
    "confbox": "^0.1.8",
    "defu": "^6.1.4",
    "hookable": "^5.5.3",
    "pathe": "^2.0.2",
    "semver": "^7.6.3",
    "type-fest": "^4.33.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "es-toolkit": "^1.31.0"
  }
}
