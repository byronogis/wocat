{
  "name": "@wocat/core",
  "type": "module",
  "version": "1.0.3",
  "description": "Core for wocat",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/wocat#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/wocat.git"
  },
  "bugs": "https://github.com/byronogis/wocat/issues",
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "@types/semver": "catalog:",
    "c12": "catalog:",
    "confbox": "catalog:",
    "defu": "catalog:",
    "hookable": "catalog:",
    "pathe": "catalog:",
    "semver": "catalog:",
    "type-fest": "catalog:",
    "yaml": "catalog:"
  },
  "devDependencies": {
    "es-toolkit": "catalog:"
  }
}
